<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<LangVersion>10.0</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<ApplicationVersion>1.0.0</ApplicationVersion>
		<ApplicationRevision>0</ApplicationRevision>
		<Version>1.0.0</Version>
		<IsTrimmable>true</IsTrimmable>
		<Description>Provides the Bootstrap icon library packaged as Blazicons, SVG icon components for Blazor.</Description>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="Blazicons" Version="0.1.3-alpha" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Blazicons.Bootstrap.Generating\Blazicons.Bootstrap.Generating.csproj" OutputItemType="Analyzer" ReferenceOutputAssembly="false" />
	</ItemGroup>

</Project>
